#include <string.h>
#include <stdlib.h>
bool isValid(char* s)
{
    int len = strlen(s);
    char *stack = (char*)malloc(len); 
    int top = -1;
    for(int i = 0; i < len; i++)
    {
        char ch = s[i];
        if(ch=='(' || ch=='{' || ch=='[')
        {
            stack[++top] = ch;
        }
        else if(ch==')' || ch=='}' || ch==']')
        {
            if(top == -1) 
            {
                free(stack);
                return 0;
            }

            char last = stack[top--];

            if ((ch == ')' && last != '(') ||
                (ch == '}' && last != '{') ||
                (ch == ']' && last != '[')) {
                free(stack);
                return 0;
            }
        }
    }

    int a = (top == -1);
    free(stack);
    return a;
}
