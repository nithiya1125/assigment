int maxProfit(int* prices, int pricesSize)
{
    if(pricesSize == 0)
    {
        return 0;
    }
    int minprices = prices[0];
    int maxprofit = 0;
    for(int i=0;i<pricesSize;i++)
    {
        if(prices[i] < minprices)
        {
            minprices = prices[i];
        }
        else
        {
            int profit = prices[i] - minprices;
            if(profit > maxprofit)
            {
                maxprofit = profit;
            }
        }
    }
    return maxprofit;
}
